{
    "manifest_version": 3,
    "name": "번역!!!",
    "version": "1.0",
    "description": "translation !!! ",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_icon":"icon3.png",
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }